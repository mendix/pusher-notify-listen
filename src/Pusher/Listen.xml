<?xml version="1.0" encoding="utf-8" ?>
<widget id="Pusher.widget.Listen" needsEntityContext="true" offlineCapable="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>Pusher Listen</name>
    <description>Listen to Notify server action and perform client side action</description>
    <icon></icon>
    <properties>
        <property key="authEndpoint" type="string" defaultValue="rest/pusher/auth" required="true">
                <caption>Auth path</caption>
                <description>Authentication endpoint path</description>
	</property>
        <property key="keyEndpoint" type="string" defaultValue="rest/pusher/key" required="true">
                <caption>Key path</caption>
                <description>Authentication key endpoint path</description>
        </property>
        <property key="actionList" type="object" isList="true">
            <caption>List</caption>
            <category>Action</category>
            <description/>
            <properties>
                <property key="actionName" type="string" defaultValue="">
                    <caption>Action name</caption>
                    <category>Action</category>
                    <description>The name should match the with the 'Notify' action parameter `ActionName`</description>
                </property>
                <property key="action" type="enumeration" defaultValue="callMicroflow">
                    <caption>Action</caption>
                    <category>Action</category>
                    <description/>
                    <enumerationValues>
                        <enumerationValue key="callMicroflow">Call a microflow</enumerationValue>
                        <enumerationValue key="callNanoflow">Call a nanoflow</enumerationValue>
                    </enumerationValues>
                </property>
                <property key="microflow" type="microflow" required="false">
                    <caption>Microflow</caption>
                    <category>Action</category>
                    <description/>
                    <returnType type="Void"/>
                </property>
                <property key="nanoflow" type="nanoflow" required="false">
                    <caption>Nanoflow</caption>
                    <category>Action</category>
                    <description/>
                    <returnType type="Void"/>
                </property>
            </properties>
        </property>
    </properties>
</widget>
